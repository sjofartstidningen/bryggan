{
  "name": "bryggan",
  "version": "1.1.1",
  "license": "MIT",
  "author":
    "Adam Bergman <adam@fransvilhelm.com> (https://github.com/adambrgmn)",
  "repository": {
    "type": "git",
    "url": "https://github.com/sjofartstidningen/bryggan.git"
  },
  "homepage": "https://bryggan.sjofartstidningen.se",
  "bugs": {
    "url": "https://github.com/sjofartstidningen/bryggan/issues"
  },
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build:headers": "node ./scripts/generate-headers.js",
    "build:app":
      "REACT_APP_BUILD_DATE=$(date -u +'%Y-%m-%dT%H:%M:%SZ') react-scripts build",
    "build": "yarn run build:app && yarn run build:headers",
    "test": "react-scripts test --env=jsdom",
    "test:no-watch": "CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "flow": "flow",
    "format": "prettier 'src/**/*' --write",
    "validate": "yarn lint && yarn test && yarn build",
    "storybook": "start-storybook -p 9009 -s public"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "firebase": "^5.0.2",
    "formik": "^0.11.11",
    "intersection-observer": "^0.5.0",
    "lodash.memoize": "^4.1.2",
    "lodash.padstart": "^4.6.1",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "md5": "^2.2.1",
    "mitt": "^1.1.3",
    "normalize.css": "^8.0.0",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "qs": "^6.5.2",
    "raf-schd": "^2.1.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-loadable": "^5.4.0",
    "react-pdf": "^3.0.5",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "styled-components": "^3.2.6",
    "styled-components-modifiers": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.4",
    "@storybook/addon-links": "^3.4.4",
    "@storybook/addons": "^3.4.4",
    "@storybook/react": "^3.4.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.72.0",
    "husky": "^1.0.0-rc.4",
    "jest-enzyme": "^6.0.0",
    "jest-styled-components": "^5.0.1",
    "lint-staged": "^7.1.0",
    "nock": "^9.2.5",
    "prettier": "^1.12.1",
    "react-test-renderer": "^16.3.2",
    "react-testing-library": "^1.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
